{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "warn",
        "no-undef": "warn",
        "no-unused-expressions": "warn",
        "no-mixed-spaces-and-tabs": 0,
        "new-cap": 0
    }
}
